<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shop.mapper.Member_tblMapper">
    
    <!-- 회원가입 -->
	<insert id="insert" parameterType="Member_tbl">
		INSERT INTO member_tbl VALUES (#{mem_id},#{mem_pwd},#{mem_email},#{mbti},#{nickname})
	</insert>

	<!-- 로그인 -->
	<select id="select" parameterType="String" resultType="Member_tbl">
		select * from member_tbl where mem_id=#{mem_id}
	</select>


	<update id="update">
		
	</update>

	<delete id="delete">
		
	</delete>
	
	<select id="selectall" resultType="Member_tbl">
		
	</select>
	
	

</mapper>
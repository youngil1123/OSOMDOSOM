<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shop.mapper.BoardMapper">

	<insert id="insert" parameterType="Board">
		insert into board values(#{board_no},#{con_no},#{mem_no},#{title},#{review},#{partner},#{writedate},#{secret_memo},#{star_rate})
		
		
	</insert>

	<select id="select" parameterType="Integer" resultType="Board">
		select * from board where board_no=#{board_no} and mem_no=#{mem_no} and title=#{title} and partner=#{partner} and secret_memo=#{secret_memo} and star_rate=#{star_rate} 
	</select>


	<update id="update">
		
	</update>

	<delete id="delete">
		
	</delete>
	
	<select id="selectall" resultType="Board">
		
	</select>
	<select id="searchmylist" parameterType="Integer" resultType="Board"  >
		select * from board where mem_no=#{mem_no}
	</select>
	
	

</mapper>